{
  "name": "classicrt",
  "version": "1.0",
  "description": "add a classic RT button to tweets",
  "icons": {
    "32" : "icon32.png",
    "48" : "icon48.png",
    "128" : "icon128.png"
  },
  "permissions": [
    "http://*.twitter.com/*"
  ],
  "content_scripts": [
    {
        "matches":["http://twitter.com/","http://twitter.com/home"],
        "js": [
            "jquery.js",
            "script.js"
        ]
    }    
  ]    
}
